FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -2 0 0 0 0];
internalField   uniform 0.0; 
boundaryField
{
    WALL_TOP
    {
        type            zeroGradient;
    }
    WALL_BOTTOM
    {
        type            zeroGradient;
    }
    frontAndBackPlanes
    {
        type            zeroGradient;
    }
    INOUT_CYC
    {
        type            cyclic;
        value           uniform 0.0; 
    }
}
